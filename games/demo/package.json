    "postversion": "git push && git push --tags",
    "upload": "surge public --domain spibo.surge.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fleveillee/canvas-test.git"
  },
  "author": "Frédéric Léveillée",
